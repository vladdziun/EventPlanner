@model LoginUser
<form asp-action="Login" asp-controller="Home" method="post">
        <span asp-validation-for="Email"></span><br>
        <label asp-for="Email"></label><br>
        <input asp-for="Email"><br>
        <span asp-validation-for="Password"></span><br>
        <label asp-for="Password"></label><br>
        <input asp-for="Password"><br>
<button>LogIn</button>
</form>
<p><a asp-controller="Home" asp-action="LoginGoogle">Login with google</a></p>
<ul>
    @if( Context.User.Identity.IsAuthenticated)
    {
        <li>
            <a>Hello @User.Identity.Name</a>
        </li>
        <li>
            <a asp-controller="Home" asp-action="LogoutGoogle">Logout Google</a>
        </li>
    }
</ul>